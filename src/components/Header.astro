            document.addEventListener('DOMContentLoaded', () => {
              const myElement = document.querySelector('hamburger');
              if (myElement) { // Always good to add a null check
                myElement.addEventListener('click', () => {
                  document.addEventListener('DOMContentLoaded', () => {
                    const mynElement = document.querySelector('nav');
                    if (mynElement) {
                      mynElement.classList.toggle('hidden');
